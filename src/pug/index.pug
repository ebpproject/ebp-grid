doctype html
html(lang="es")
	head
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width,initial-scale=1")
		title= title
		//- All style sheets
		each css in files.CSS
			link(rel="stylesheet", href=`../${css}`)

	body
		//- All javacript
		each js in files.JS
			script(src=`../${js}`)
